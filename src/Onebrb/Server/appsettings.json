{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "NLog": "Server=(localdb)\\MSSQLLocalDB;Initial Catalog=Onebrb_Logs;Integrated Security=SSPI;"
  },
  "IdentityServer": {
    "Clients": {
      "Onebrb.Client": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*",

  // Custom settings
  "OnebrbOptions": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=OnebrbIdentity;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}